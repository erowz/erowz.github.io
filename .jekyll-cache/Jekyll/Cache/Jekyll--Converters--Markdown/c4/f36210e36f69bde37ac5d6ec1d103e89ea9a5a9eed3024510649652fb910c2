 <span class="o">&gt;&gt;</span> <span class="o">&lt;</span><span class="n">__main__</span><span class="o">.</span><span class="n">Point</span> <span class="nb">object</span> <span class="n">at</span> <span class="mh">0x11520b3d0</span><span class="o">&gt;</span>

 <span class="n">p1</span><span class="o">.</span><span class="n">print_point</span><span class="p">()</span>
